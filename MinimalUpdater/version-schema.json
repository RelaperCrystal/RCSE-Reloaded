{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Version Schema for RCSE",

  "type": "object",

  "properties": {
    "name": {
      "type": "string",
      "description": "The name of release."
    },
    "stage": {
      "type": "string",
      "description": "The stage of release."
    },
    "release": {
